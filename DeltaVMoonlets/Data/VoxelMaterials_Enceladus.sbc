  <VoxelMaterials>
    
	<!--EnceladusSnow01-->	
	<VoxelMaterial xsi:type="MyObjectBuilder_Dx11VoxelMaterialDefinition">	<!--just alien snow, but with rocks-->
      <Id>
        <TypeId>VoxelMaterialDefinition</TypeId>
        <SubtypeId>EnceladusSnow01</SubtypeId>
      </Id>
	  <SpawnsInAsteroids>false</SpawnsInAsteroids>
      <SpawnsFromMeteorites>false</SpawnsFromMeteorites>
      <MaterialTypeName>Snow</MaterialTypeName>
      <MinedOre>Ice</MinedOre>
      <MinedOreRatio>2</MinedOreRatio>
      <CanBeHarvested>true</CanBeHarvested>
      <IsRare>false</IsRare>
      
      <VoxelHandPreview>Textures\Voxels\Snow_01_Thumbnail.dds</VoxelHandPreview>
      <ColorMetalXZnY>Textures\Voxels\Enceladus\EnceladusSnow_cm.dds</ColorMetalXZnY>
      <NormalGlossXZnY>Textures\Voxels\AlienSnow_Far1_ng.dds</NormalGlossXZnY>
      <ExtXZnY>Textures\Voxels\Dummy_add.dds</ExtXZnY>
      
      <!--Far1 Texture-->
      <ColorMetalXZnYFar1>Textures\Voxels\Enceladus\EnceladusSnow_cm.dds</ColorMetalXZnYFar1>
      <NormalGlossXZnYFar1>Textures\Voxels\AlienSnow_Far1_ng.dds</NormalGlossXZnYFar1>
      <ExtXZnYFar1>Textures\Voxels\Dummy_add.dds</ExtXZnYFar1>

      <!--Far2 Texture-->
      <ColorMetalXZnYFar2>Textures\Voxels\Enceladus\EnceladusSnow_cm.dds</ColorMetalXZnYFar2>
      <NormalGlossXZnYFar2>Textures\Voxels\AlienSnow_Far2_ng.dds</NormalGlossXZnYFar2>
      <ExtXZnYFar2>Textures\Voxels\Dummy_add.dds</ExtXZnYFar2>

      <ExtDetailScale>0.005</ExtDetailScale>
      <Scale>500</Scale>
      <Far1Distance>50</Far1Distance>
      <Far2Distance>500</Far2Distance>
      <Far1Scale>300</Far1Scale>
      <Far2Scale>2000</Far2Scale>

      <Far3Distance>6000</Far3Distance>
      <Far3Color>
        <X>0.63</X>
        <Y>0.63</Y>
        <Z>0.73</Z>
        <W>1.0</W>
      </Far3Color>
	  
	  
      <FoliageColorTextureArray>
		<Color>Textures\Voxels\Snow_01_ForAxisXZ_cm.dds</Color>
		<Color>Textures\Voxels\Enceladus\Enceladus_Ice_02_cm.dds</Color>
      </FoliageColorTextureArray>
      <FoliageNormalTextureArray>
		<Normal>Textures\Voxels\Snow_01_ForAxisXZ_ng.dds</Normal>
		<Normal>Textures\Voxels\IceEuropa_ng.dds</Normal>
      </FoliageNormalTextureArray>
      <FoliageDensity>0.6</FoliageDensity>
      <FoliageScale>
        <X>1</X>
        <Y>2</Y>
      </FoliageScale>
      <FoliageRandomRescaleMult>2.2</FoliageRandomRescaleMult>
      <FoliageType>1</FoliageType>
      

      <Friction>0.65</Friction>
      <Restitution>0.15</Restitution>
      <ColorKey Hex="#FFFFFF"/>
    </VoxelMaterial>
	
